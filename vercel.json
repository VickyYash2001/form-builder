{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/angular",
      "config": {
        "outputDirectory": "dist/form-builder/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/index.html"
    },
    {
      "src": "/assets/(?<assetPath>.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    }
  ]
}
